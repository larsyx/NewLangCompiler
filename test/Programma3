
integer result << 1;




def fibonacci(integer i): integer{

    var c1<< 1, c2<< 2;

    if (i=0) then {
        return 0;
    }
    if(i=1) then{
        return 10;
    }
    else{
        integer i1 << i-c1;
        integer i2 << i-c2;
        integer r1,r2;
        r1 << fibonacci(i1);
        r2 << fibonacci(i2);
        return r1 + r2;
    }
}


start:
def main():void{
    result << fibonacci(5);

    ("numero di fibonacci: ", result) -->!;
    return;
}